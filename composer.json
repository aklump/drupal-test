{
    "name": "aklump\/drupal-test",
    "description": "A solution for testing Drupal 7 using PhpUnit.",
    "license": "BSD-3-Clause",
    "homepage": "https:\/\/github.com\/aklump\/drupal-test",
    "autoload": {
        "psr-4": {
            "AKlump\\DrupalTest\\": "src\/DrupalTest"
        }
    },
    "scripts": {
        "pre-install-cmd": "AKlump\\DrupalTest\\Config::addConfig",
        "pre-update-cmd": "AKlump\\DrupalTest\\Config::addConfig",
        "pre-autoload-dump": "AKlump\\DrupalTest\\Config::addConfig"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/michailw/phpunit-mink"
        }
    ],
    "require": {
        "php": ">=7.0",
        "mwojtowicz/phpunit-mink": "^2.2|^3.0",
        "aklump/phpunit-extras": "^2.0",
        "drupal\/drupal-driver": "^1.4",
        "guzzlehttp\/guzzle": "^6.3",
        "justinrainbow\/json-schema": "^5.2",
        "mockery\/mockery": "^1.0",
        "sunra\/php-simple-html-dom-parser": "^1.5",
        "wikimedia\/composer-merge-plugin": "^1.4",
        "aklump/manual-test": "^1.2",
        "kevinlebrun/password.php": "^0.1",
        "behat/mink-goutte-driver": "^1.2",
        "symfony/browser-kit": "^3.4",
        "symfony/yaml": "^3.0|^4.0",
        "erusev\/parsedown": "^1.7",
        "php-mime-mail-parser/php-mime-mail-parser": "^4.0"
    },
    "authors": [
        {
            "name": "Aaron Klump",
            "email": "sourcecode@intheloftstudios.com"
        }
    ]
}
